    "electron": "^7.1.7",
    "electron-builder": "^21.2.0",
    "next": "latest",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tailwindcss": "^2.0.1"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "postcss": "^8.2.1",
    "pretty-bytes": "^5.4.1",
    "sass": "^1.30.0",
    "systeminformation": "^4.31.0",
    "tailwindcss-filters": "^3.0.0"
  },
  "license": "MIT"
}
